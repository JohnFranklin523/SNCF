/* Choose between front, middle and back parts */
switch(FEAT_TRAINS, SELF, sw_TGV_PSE_graphics, position_in_articulated_veh) {
    0: set_TGV_PSE_front;
    1: set_TGV_PSE_1st_1;
    2: set_TGV_PSE_1st_2;
    3: set_TGV_PSE_1st_3;
    4: set_TGV_PSE_bar;
    5: set_TGV_PSE_2nd_4;
    6: set_TGV_PSE_2nd_3;
    7: set_TGV_PSE_2nd_2;
    8: set_TGV_PSE_2nd_1;
    9: set_TGV_PSE_rear;
}

switch(FEAT_TRAINS, SELF, sw_TGV_PSE_articulated_part, extra_callback_info1) {
    1 .. 9: return item_TGV_PSE;
    return CB_RESULT_NO_MORE_ARTICULATED_PARTS;
}

switch(FEAT_TRAINS, SELF, sw_TGV_PSE_check_length, num_vehs_in_consist) {
    0 .. 20: return CB_RESULT_NO_TEXT;
    return string(ERR_TGV_PSE_TOO_LONG);
}

switch(FEAT_TRAINS, SELF, sw_TGV_PSE_can_attach_wagon, vehicle_type_id) {
    item_TGV_PSE: CB_RESULT_ATTACH_ALLOW;
    return string(ERR_TGV_PSE_CANNOT_ATTACH_OTHER);
}

switch(FEAT_TRAINS, SELF, sw_TGV_PSE_length, position_in_articulated_veh) {
    1: 7;
    3: 7;
    5: 7;
    7: 7;
    default: 8;
}

switch(FEAT_TRAINS, SELF, sw_TGV_PSE_capacity, position_in_articulated_veh) {
    1: 32;
    2 .. 3: 40;
    5 .. 7: 66;
    8: 60;
    default: 0;
}

switch(FEAT_TRAINS, SELF, sw_TGV_PSE_power, current_railtype) {    
    ELEC: 8650;
    AC25: 8650;
    DC15: 4157;
    AC15: 3755;
    default: 8650;
}

item(FEAT_TRAINS, item_TGV_PSE) {
    property {
        name:                         string(STR_SNCF_TRAIN_TGV_PSE_NAME);
        climates_available:           (param_allow_electric) ? bitmask(CLIMATE_TEMPERATE, CLIMATE_ARCTIC, CLIMATE_TROPICAL) : NO_CLIMATE;
        introduction_date:            date(1978, 1, 1);
        model_life:                   23;
        vehicle_life:                 20;
        reliability_decay:            19;
        refittable_cargo_classes:     bitmask(CC_PASSENGERS);
        non_refittable_cargo_classes: bitmask();
        loading_speed:                LOADING_FAST; 
        cost_factor:                  185;
        running_cost_factor:          170;
        sprite_id:                    SPRITE_ID_NEW_TRAIN;
        speed:                        270 km/h;
        misc_flags:                   bitmask(TRAIN_FLAG_MU, TRAIN_FLAG_AUTOREFIT);
        refit_cost:                   0;
        track_type:                   (!param_allow_voltage) ? ELEC : AC25AC15DC15;
        ai_special_flag:              AI_FLAG_PASSENGER;
        power:                        6420 kW;
        running_cost_base:            RUNNING_COST_ELECTRIC;
        dual_headed:                  0;
        weight:                       285 ton;
        ai_engine_rank:               0;
        engine_class:                 ENGINE_CLASS_ELECTRIC;
        extra_power_per_wagon:        0 kW;
        tractive_effort_coefficient:  0.5;
        air_drag_coefficient:         0.06;
        extra_weight_per_wagon:       0 ton;
        bitmask_vehicle_info:         0;
        cargo_capacity:               1;
        cargo_age_period:             250;
    }
    graphics {
        default:                      sw_TGV_PSE_graphics;
        purchase:                     set_TGV_PSE_purchase;
        additional_text:              return string(STR_SNCF_TRAIN_TGV_PSE_ADDITIONAL_TEXT);
        articulated_part:             sw_TGV_PSE_articulated_part;
        can_attach_wagon:             sw_TGV_PSE_can_attach_wagon;
        start_stop:                   sw_TGV_PSE_check_length;
        visual_effect_and_powered:    return (position_in_consist % 10 == 0) ?
                                          visual_effect_and_powered(VISUAL_EFFECT_ELECTRIC, 2, DISABLE_WAGON_POWER) :
                                          visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
        length:                       sw_TGV_PSE_length;
        cargo_capacity:               sw_TGV_PSE_capacity;
        purchase_cargo_capacity:      return 37;
        power:                        sw_TGV_PSE_power;
    }
}